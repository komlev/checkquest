    "keyux": "^0.11.3",
    "nanoid": "^5.1.6",
    "nanostores": "^0.11.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "theme-change": "^2.5.0",
    "wouter": "^3.7.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@biomejs/biome": "2.3.2",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@playwright/test": "^1.56.1",
