    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "4.0.6",
    "@vitest/ui": "^4.0.6",
    "daisyui": "^5.4.2",
    "globals": "^15.15.0",
    "jsdom": "^26.1.0",
    "lefthook": "^2.0.2",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^4.0.6"
