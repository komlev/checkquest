    "typecheck": "tsc -b --noEmit",
    "lint-commit": "commitlint --from HEAD~1 --to HEAD",
    "preview": "vite preview",
    "test": "vitest ./src",
    "test-e2e": "playwright test"
  },
  "dependencies": {
    "@nanostores/persistent": "^0.10.2",
