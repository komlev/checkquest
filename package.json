{
  "name": "checkquest",
  "private": true,
  "version": "1.1.3",
  "type": "module",
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "predev": "node scripts/generate-templates.js",
    "dev": "vite",
    "prebuild": "node scripts/generate-templates.js",
    "build": "tsc -b && vite build",
    "lint": "biome check",
