    "es-toolkit": "^1.44.0",
    "keyux": "^0.11.3",
    "nanoid": "^5.1.6",
    "nanostores": "^0.11.4",
    "preact": "^10.28.2",
    "theme-change": "^2.5.0",
    "wouter": "^3.9.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@biomejs/biome": "2.3.2",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@playwright/test": "^1.57.0",
    "@preact/preset-vite": "^2.10.2",
