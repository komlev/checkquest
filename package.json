    "@vitest/coverage-v8": "4.0.6",
    "@vitest/ui": "^4.0.17",
    "daisyui": "^5.5.14",
    "globals": "^15.15.0",
    "jsdom": "^26.1.0",
    "lefthook": "^2.0.15",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-bundle-analyzer": "^1.3.2",
    "vitest": "^4.0.17"
  }
